function checkbox_setter(k){
    let wywg=document.querySelector('#wywg'+k);
    if(wywg){
        if(wywg_set[k]==1){
            wywg.checked=true; }
        else{
            wywg.checked=false; }

        wywg.onclick=function(){
            if(wywg_set[k]==1){
                wywg.checked=false;
                wywg_set[k]=0; }
            else{
                wywg.checked=true;
                wywg_set[k]=1; }
            let write_json=JSON.stringify(wywg_set);
            localStorage.setItem('Wywg_set', write_json); // ãƒ­ãƒ¼ã‚«ãƒ«ã‚¹ãƒˆãƒ¬ãƒ¼ã‚¸ ä¿å­˜
            renew_insert_tag(); }}}



function input_value_setter(k){
    let wywg=document.querySelector('#wywg'+k);
    if(wywg){
        wywg.value=wywg_set[k];

        wywg.onchange=function(){
            wywg_set[k]=wywg.value;
            let write_json=JSON.stringify(wywg_set);
            localStorage.setItem('Wywg_set', write_json); // ãƒ­ãƒ¼ã‚«ãƒ«ã‚¹ãƒˆãƒ¬ãƒ¼ã‚¸ ä¿å­˜
            renew_insert_tag(); }}}



