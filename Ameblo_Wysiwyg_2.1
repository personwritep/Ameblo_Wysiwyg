                event.stopImmediatePropagation();
                if(menu_view==0){
                    menu_view=1;
                    disp_panel();
                    l_form.style.marginLeft='10px';
                    l_form.style.marginRight='auto'; }
                else{
                    menu_view=0;
                    wywg_menu.remove();
                    l_form.style.marginLeft='auto';
                    l_form.style.marginRight='auto'; }

